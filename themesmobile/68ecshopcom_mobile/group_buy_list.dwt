<body class="eth_v1_mobile page-stores"  style="background:#f4f2f3">
  <header>
  <div class="tab_nav">
    <div class="header">
      <div class="h-left">
        <a class="sb-back" href="javascript:history.back(-1)" title="{$lang.m_166}">
        <!-- <i class="fa fa-chevron-left"></i> -->
      </a></div>
      <div class="h-mid">{$lang.group_by_goods}</div>
      <div class="h-right">
        <aside class="top_bar">
          <div onClick="show_menu();$('#close_btn').addClass('hid');" id="show_more"><a href="javascript:;"></a> </div>
        </aside>
      </div>
    </div>
  </div>
  </header>

  <div style=" width:100%; height:50px;"></div>

<!--main--->
{if $gb_list}

  <div id="tbh5v0">
      <section class="innercontent"> 
        <div class="main">
          <!-- {if $gb_list} 如果有团购活动 -->
            <div class="floor_body auction_hot">
              <div class="mc">
                <ul>
                   
                  {foreach from=$gb_list item=group_buy}
                    <section class="rzs_info">
                      <div class="index_taocan">
                        <a href="{$group_buy.url}"> 
                          <div class="img-box-groupby">
                            <img src="{$img_url}{$group_buy_list}{$group_buy.goods_thumb}" class="B_eee">
                          </div>
                        </a>
                      </
                      <div class="index_taocan">
                        <a href="{$group_buy.url}"> 
                          <dl>
                            <div class="desc-price">
                              <div>{$group_buy.goods_name|escape:html}</div>
                              <div style="color: red;"><b>{$group_buy.formated_cur_price}</b></div>
                            </div>
                          </dl>
                        </a>
                      </div>
                      <div class="s_dianpu">
                        <span>
                          <a href="{$group_buy.url}" style=" margin-left:15%" class="btns">{$lang.m_363}</a>
                        </span>
                      </div>
                    </section>
                  {/foreach}
                </ul>
              </div>
            </div>
          <!-- {/if} --> 
        </div>
      </section>
    </div>

    <!-- #BeginLibraryItem "/library/pages.lbi" --><!-- #EndLibraryItem -->
{else}
  <div class="touchweb-com_searchListBox" id="goods_list">
    <div class="new_prom2">
      <strong>{$lang.m_364}</strong>
    </div>  
  </div>
{/if}

