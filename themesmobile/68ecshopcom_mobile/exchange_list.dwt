<body class="eth_v1_mobile page-exchange-list">
      <!----------------------------------header----------------------------------->
      <div class="tab_nav">
        <div class="header">
          <div class="h-left"><a class="sb-back" href="javascript:history.back(-1)" title="{$lang.m_166}"><i class="fa fa-chevron-leftt"></i></a></div>
          <div class="h-mid">{$lang.exchange}</div>
          <div class="h-right">
            <aside class="top_bar">
